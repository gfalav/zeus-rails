<p id="notice"><%= notice %></p>

<h1>Lista de Clientes</h1>

<table>
  <thead>
    <tr>
      <th>Apellido</th>
      <th>Apellido2</th>
      <th>Nombres</th>
      <th>Documento</th>
      <th>Tipodoc</th>
      <th>Emisordoc</th>
      <th>Contacto</th>
      <th>Ciiu</th>
      <th>Tcliente</th>
      <th>Direccion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.apellido %></td>
        <td><%= cliente.apellido2 %></td>
        <td><%= cliente.nombres %></td>
        <td><%= cliente.documento %></td>
        <td><%= cliente.tipodoc_id %></td>
        <td><%= cliente.emisordoc_id %></td>
        <td><%= cliente.contacto %></td>
        <td><%= cliente.ciiu_id %></td>
        <td><%= cliente.tcliente_id %></td>
        <td><%= cliente.direccion_id %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
