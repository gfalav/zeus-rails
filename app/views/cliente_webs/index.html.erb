<p id="notice"><%= notice %></p>

<h1>Listing Cliente Webs</h1>

<table>
  <thead>
    <tr>
      <th>Cliente</th>
      <th>Web</th>
      <th>Tipoweb</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cliente_webs.each do |cliente_web| %>
      <tr>
        <td><%= cliente_web.cliente_id %></td>
        <td><%= cliente_web.web %></td>
        <td><%= cliente_web.tipoweb_id %></td>
        <td><%= link_to 'Show', cliente_web %></td>
        <td><%= link_to 'Edit', edit_cliente_web_path(cliente_web) %></td>
        <td><%= link_to 'Destroy', cliente_web, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente web', new_cliente_web_path %>
