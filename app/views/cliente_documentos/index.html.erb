<p id="notice"><%= notice %></p>

<h1>Listing Cliente Documentos</h1>

<table>
  <thead>
    <tr>
      <th>Cliente</th>
      <th>Documento</th>
      <th>Tipodoc</th>
      <th>Emisordoc</th>
      <th>Vencimiento</th>
      <th>Codseg</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cliente_documentos.each do |cliente_documento| %>
      <tr>
        <td><%= cliente_documento.cliente_id %></td>
        <td><%= cliente_documento.documento %></td>
        <td><%= cliente_documento.tipodoc_id %></td>
        <td><%= cliente_documento.emisordoc_id %></td>
        <td><%= cliente_documento.vencimiento %></td>
        <td><%= cliente_documento.codseg %></td>
        <td><%= link_to 'Show', cliente_documento %></td>
        <td><%= link_to 'Edit', edit_cliente_documento_path(cliente_documento) %></td>
        <td><%= link_to 'Destroy', cliente_documento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente documento', new_cliente_documento_path %>
